inoremap jk <ESC>
let mapleader = "'"
syntax on
set enc=utf-8
set number
set mouse=a
set autoindent
set tabstop=4
set sw=4
set sts=4
set noexpandtab
set splitright
set ignorecase
set incsearch
set noswapfile
set hlsearch
colorscheme blue
set clipboard^=unnamed,unnamedplus
:tnoremap <Esc> <C-\><C-n>
